# description	: GDB - GCC debugger
# depends	: gcc binutils cmake

name=gdb
version=9999
release=git-r1
options=""
noextract=""
backup=""
source=""

build() {
  git clone https://github.com/bminor/binutils-gdb.git --depth 1 --jobs $(nproc)
	cd binutils-gdb
	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
